program Frutosdias;

uses
  System.StartUpCopy,
  FMX.Forms,
  UPrincipal in 'UPrincipal.pas' {f_principal},
  UAgendamento in 'UAgendamento.pas' {f_agendamento},
  UConfiguracoes in 'UConfiguracoes.pas' {f_configuracoes},
  UConsorcio in 'UConsorcio.pas' {f_consorcio},
  UNovo_carro in 'UNovo_carro.pas' {f_novo_carro},
  URevisoes in 'URevisoes.pas' {f_revisoes},
  UServerClasses in 'UServerClasses.pas',
  UVenda_Direta in 'UVenda_Direta.pas' {f_venda_direta};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(Tf_principal, f_principal);
  Application.CreateForm(Tf_agendamento, f_agendamento);
  Application.CreateForm(Tf_configuracoes, f_configuracoes);
  Application.CreateForm(Tf_consorcio, f_consorcio);
  Application.CreateForm(Tf_novo_carro, f_novo_carro);
  Application.CreateForm(Tf_revisoes, f_revisoes);
  Application.CreateForm(Tf_venda_direta, f_venda_direta);
  Application.Run;
end.
